{
  "update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "CxForward",
  "version": "1.1.2",
  "short_name": "CxForward",
  "author": "Jahastech",
  "description": "NxFilter's HTTPS block page displayer.",
  "icons": { "16": "16_cx.png", "48": "48_cx.png", "128": "128_cx.png" },
  "action": {
    "default_icon": "16_cx.png"
  },
  "background": {
    "service_worker": "service_worker.js"
  },
  "options_page": "options.html",
  "permissions": ["tabs", "storage", "webNavigation"],
  "host_permissions": ["*://*/*"]
}
